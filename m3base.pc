prefix=/usr/local
libdir=${prefix}/lib
includedir=${prefix}/include
ros_dir = /opt/ros/hydro

Name: m3base
Description: Base library for M3 Core.
Version: 1.5.0
Requires: yaml-cpp >= 0.2.5, protobuf >= 2.2.0a, orocos-kdl >= 1.0.2, eigen3 >= 3.0, ethercat >= 1.5.0
Libs: -L${libdir} -lm3base
Libs.private: -L/usr/realtime/lib
Cflags: -I${includedir}/m3 -Wl,-E -ffast-math -pipe -mhard-float \
	-I/usr/realtime/include \
